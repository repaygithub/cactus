{
  "name": "@repay/cactus-icons",
  "version": "0.1.0",
  "description": "Cactus design-system icons as React elements",
  "main": "index.js",
  "repository": "https://github.com/repaygithub/cactus/tree/master/modules/cactus-icons",
  "author": "REPAY",
  "license": "MIT",
  "scripts": {
    "cleanup": "rm -rf built dist",
    "build": "yarn build:ci",
    "postbuild": "node scripts/gen-docs.js",
    "build:ci": "yarn build:ts && yarn build:babel && yarn build:types",
    "build:ts": "svgr svgs -d built --config-file svgr.config.js --ext tsx --filename-case kebab",
    "postbuild:ts": "node ./scripts/post-build-ts",
    "build:babel": "NODE_ENV=production babel built -d dist -x .ts -x .tsx",
    "postbuild:babel": "node scripts/post-build-babel",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "debug": "node --inspect-brk ./node_modules/.bin/svgr svgs -d built --config-file svgr.config.js --ext tsx",
    "test": "echo no tests implemented for cactus-icons",
    "pretest:ci": "yarn build:ts",
    "test:ci": "yarn test:types",
    "test:types": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": "^16.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@repay/babel-preset": "^0.1.1",
    "@svgr/cli": "^4.1.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "styled-system": "^4.1.0"
  }
}
