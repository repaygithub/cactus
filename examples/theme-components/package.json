{
  "name": "theme-components",
  "version": "0.1.0",
  "description": "An example implementation of repay/cactus-web using @repay/cactus-theme",
  "private": true,
  "scripts": {
    "cleanup": "rm -rf dist",
    "start": "yarn dev",
    "dev": "repay-scripts dev --config ./repay-scripts.config.js -p 3436 src/index.tsx",
    "build": "repay-scripts build --config ./repay-scripts.config.js src/index.tsx",
    "test": "yarn test:types",
    "test:ci": "yarn test",
    "test:local": "yarn test",
    "test:types": "tsc -p ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@repay/scripts": "^2.0.0",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "csstype": "^2.6.11",
    "prop-types": "^15.7.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@repay/cactus-icons": "^2.0.0",
    "@repay/cactus-theme": "^1.0.0",
    "@repay/cactus-web": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  }
}
