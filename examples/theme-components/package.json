{
  "name": "theme-components",
  "version": "0.1.0",
  "description": "An example implementation of repay/cactus-web using @repay/cactus-theme",
  "private": true,
  "scripts": {
    "cleanup": "rm -rf dist",
    "dev": "repay-scripts dev --config ./repay-scripts.config.js -p 3436 src/index.tsx",
    "build": "repay-scripts build --config ./repay-scripts.config.js src/index.tsx",
    "test": "yarn test:types",
    "test:ci": "yarn test",
    "test:types": "tsc -p ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@repay/scripts": "^0.2.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@repay/cactus-icons": "^0.4.1",
    "@repay/cactus-theme": "^0.4.1",
    "@repay/cactus-web": "^0.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.1.2"
  },
  "resolutions": {
    "styled-components": "^4.1.2"
  }
}
