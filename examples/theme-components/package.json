{
  "name": "theme-components",
  "version": "0.1.0",
  "description": "An example implementation of repay/cactus-web using @repay/cactus-theme",
  "private": true,
  "scripts": {
    "cleanup": "rm -rf dist",
    "start": "yarn dev",
    "dev": "repay-scripts dev --config ./repay-scripts.config.js -p 3436 src/index.tsx",
    "build": "repay-scripts build --config ./repay-scripts.config.js src/index.tsx",
    "test": "yarn test:types",
    "test:ci": "yarn test",
    "test:local": "yarn test",
    "test:types": "tsc -p ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@repay/scripts": "^2.0.2",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "csstype": "^3.0.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@repay/cactus-icons": "^2.1.6",
    "@repay/cactus-theme": "^2.0.2",
    "@repay/cactus-web": "^6.0.0-beta.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "styled-components": "^5.2.1"
  }
}
