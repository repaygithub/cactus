{
  "extends": ["@repay/eslint-config"],
  "overrides": [
    {
      "files": ["**/*.{js,jsx}"],
      "rules": {
        "no-unused-vars": "off",
        "no-undef": "off"
      }
    },
    {
      "files": ["**/*.{ts,tsx}"],
      "extends": ["@repay/eslint-config/ts"],
      "rules": {
        "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/ban-ts-comment": "off",
        "@typescript-eslint/no-empty-interface": "off"
      }
    }
  ]
}
