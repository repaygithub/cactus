    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-motion": "^0.0.29",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-export-metadata": "^2.3.0",
    "babel-plugin-styled-components": "^1.12.0",
    "chokidar": "^3.5.1",
    "cpy": "^8.1.1",
    "fast-glob": "^3.2.5",
