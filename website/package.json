  "private": true,
  "scripts": {
    "cleanup": "gatsby clean",
    "update:stories-cache": "node tools/build-storybook-cache.js",
    "predev": "yarn update:stories-cache",
    "start": "yarn dev",
    "dev": "gatsby develop --https --port 8001",
    "debug": "node --nolazy --inspect-brk node_modules/.bin/gatsby develop --https --port 8001",
    "prebuild": "yarn cleanup && yarn update:stories-cache",
    "build": "gatsby build --prefix-paths",
    "postbuild": "node tools/build-storybooks",
    "test": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "test:ci": "yarn test",
    "test:local": "yarn test"
  },
  "devDependencies": {
