    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/react-motion": "^0.0.29",
    "@types/styled-components": "5.1.9",
    "babel-plugin-export-metadata": "^2.3.0",
    "babel-plugin-styled-components": "^1.13.1",
    "chokidar": "^3.5.2",
    "cpy": "^8.1.2",
    "esm": "^3.2.25",
